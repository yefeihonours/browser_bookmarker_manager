{
      "name": "书签检索器",
      "icons":
      {
          "16": "icon.png",
          "48": "icon.png",
          "128": "icon.png"
      },
	  "version": "0.1",
	  "description": "书签检索器，检索收藏的网址和文件夹，输入检索关键字后，按回车键发起检索",
	  "permissions": [
	    "bookmarks"
	  ],
	  "browser_action": {
	      "default_title": "书签检索器",
	      "default_icon": "icon.png",
	      "default_popup": "popup.html"
	  },
      "manifest_version": 2,
	  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}